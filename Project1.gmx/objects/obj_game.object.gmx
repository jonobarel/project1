            <string>///Game Loop
if(room_get_name(room) == 'rm_game') {
    if (instance_number(obj_asteroid) == 0)
    {
        global.level+=1;
        for (i=0; i&lt;global.level*1.5+1; i+=1)
        {
            radius = irandom(500)+100;
            angle = irandom(360)/(0.5*pi);
        
            //when I was using a random radius, asteroids were getting created far off-screen. This will ensure that their distance is capped.
            x_offset = min(room_width/2,(irandom(1)*2-1)*radius*cos(angle));
